  <BranchConstraint Name="SystemRequirement_db_5.0" >

    <!-- OS version check -->
    <Constraint Type="OSVER"
                FallbackAction="SKIP"
		Qualifier="EQUAL" >
      <RefData>Windows 2000</RefData>
    </Constraint>

    <!-- service pack 1 or higher -->
    <Constraint Type="OSPATCH"
                FallbackAction="STOP"
		Qualifier="GTE" >
      <KeyData>1</KeyData>
    </Constraint>

    <!-- 512MB RAM -->
    <!-- Memory check -->
    <Constraint Type="TOTALMEM"
                FallbackAction="STOP"
		Qualifier="GTE" >
      <RefData>536870912</RefData>
    </Constraint>

    <!-- Swap check -->
